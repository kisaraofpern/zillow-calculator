<form action="/calculator" method="post">

  <label for="Address">Address</label>
  <input
    type="text"
    id="address"
    name="address"/>

  <label for="City">City</label>
  <input
    type="text"
    id="city"
    name="city"
  />

  <label for="State">State</label>
  <input
    type="state"
    id="state"
    name="state"
  />

  <label for="Zip">Zip Code</label>
  <input
    type="text"
    id="zip"
    name="zip"/>
  <input type="submit" value="Submit" />
  
</form>
